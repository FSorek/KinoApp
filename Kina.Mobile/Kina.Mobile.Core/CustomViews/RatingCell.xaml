<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Kina.Mobile.Core.CustomViews.RatingCell"
             x:Name="This">
    <ContentView.Content>
        <Grid>
            <Grid.BindingContext>
                <x:Reference Name="This"/>
            </Grid.BindingContext>
            <Image Source="RatingCellOutline.png"/>
            <Image IsVisible="{Binding IsMarked}"
                   Source="RatingCell.png"/>
        </Grid>
    </ContentView.Content>
</ContentView>
    